import turtle

my_turtle = turtle.Turtle()

def square(length, angle):
    
    my_turtle.forward(length)
    my_turtle.right(angle)
    my_turtle.forward(length)
    my_turtle.right(angle)
    my_turtle.forward(length)
    my_turtle.right(angle)
    my_turtle.forward(length)
    my_turtle.right(angle)
    

for i in range(100):
    square(90,90)
    my_turtle.right(12)






